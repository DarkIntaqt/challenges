{
   "extends": "next/core-web-vitals",
   "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "no-unused-vars": "warn",
      "unused-imports/no-unused-imports": "error",
      "react/no-unknown-property": [
         1,
         {
            "ignore": ["bar-id"]
         }
      ]
   },
   "plugins": ["unused-imports"]
}
